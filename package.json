{
	"name": "tsrq",
	"author": "Arnav Gosain",
	"version": "0.0.4",
	"license": "MIT",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"start": "tsdx watch",
		"build": "tsdx build",
		"test": "tsdx test",
		"lint": "tsdx lint",
		"prepare": "tsdx build",
		"size": "size-limit",
		"analyze": "size-limit --why",
		"format": "prettier --write ./src/**/*.{js,jsx,ts,tsx}"
	},
	"peerDependencies": {
		"react": ">=16",
		"react-query": ">=3 <=3.25.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "tsdx lint"
		}
	},
	"prettier": {
		"printWidth": 80,
		"semi": true,
		"trailingComma": "all",
		"useTabs": true,
		"tabWidth": 2
	},
	"module": "dist/tsrq.esm.js",
	"size-limit": [
		{
			"path": "dist/tsrq.cjs.production.min.js",
			"limit": "10 KB"
		},
		{
			"path": "dist/tsrq.esm.js",
			"limit": "10 KB"
		}
	],
	"devDependencies": {
		"@size-limit/preset-small-lib": "^5.0.4",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.0.0",
		"@testing-library/user-event": "^13.2.1",
		"@types/react": "^17.0.26",
		"@types/react-query": "^1.2.9",
		"husky": "^7.0.2",
		"jest": "^27.1.0",
		"size-limit": "^5.0.4",
		"tsdx": "^0.14.1",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"react": "latest",
		"react-query": "^3.24.0"
	}
}
